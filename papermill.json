  "dependencies": {
    "docco": "~0.6.2",
    "npm": "~1.3.7"
  },
  "scripts": {
    "precompile": "./node_modules/docco/bin/docco -o CODE/mill/docs CODE/mill/*.js CODE/mill/lib/*.js"
  },
