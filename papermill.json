{
  "name": "papermill",
  "author": "Max F. Albrecht",
  "description": "Bachelor Thesis (Media Design), Bauhaus-University Weimar",
  "version": "0.0.0",
  "private": true,
  "bibliography": "bibliography.bib",
  "input": {
    "list": [
      "Thesis",
      {
        "path": "CODE/C0DE.md",
        "title": "Papermill: C0DE",
